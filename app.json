{
   "expo": {
      "name": "weather",
      "slug": "weather",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/weather/oblachno.png",
      "userInterfaceStyle": "light",
      "splash": {
         "image": "./assets/splash2.png",
         "resizeMode": "cover"
      },
      "updates": {
         "fallbackToCacheTimeout": 0
      },
      "assetBundlePatterns": [
         "**/*"
      ],
      "ios": {
         "supportsTablet": true
      },
      "android": {
         "adaptiveIcon": {
            "foregroundImage": "./assets/weather/oblachno.png",
            "backgroundColor": "#9775A5"
         },
         "package": "weather.app"
      },
      "web": {
         "favicon": "./assets/favicon.png"
      }
   }
}